swiper,swiper-item image{
width:100%;
height:465rpx;
}
.head{
  
    height: 80rpx;
    background: #f2f2f6;
    line-height: 80rpx;
    color: #999;
    width: 100%;
    font-size: 30rpx;
}
dl{
  width: 100%;
  margin: 0 auto;
}

.clearfix {
    zoom: 1;
}
dd{
  float: left;
  display: block;
  width: 25%;
  text-align: center;
   font-size: 28rpx;

}
ul{
  list-style: none;
}
li{
    height: 75rpx;
    line-height: 75rpx;
    display: block;
}
ul li view {
   float: left;
  width: 25%;
  text-align: center;
   font-size: 28rpx;
}
ul li:nth-child(even) {
  background-color:#f2f2f6;
}


.audio{
  width: 102rpx;
  height:110rpx;
  position: absolute;
  left: 50%;
  margin-left: -51rpx;
  top:180rpx;
  opacity: 0.6;
}